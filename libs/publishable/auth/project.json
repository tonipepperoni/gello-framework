{
  "name": "gello-auth",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/publishable/auth/src",
  "projectType": "library",
  "tags": ["publishable"],
  "targets": {
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "angular",
        "tagPrefix": "auth-v",
        "postTargets": ["gello-auth:publish"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "npm publish --access public",
        "cwd": "libs/publishable/auth"
      }
    }
  }
}
