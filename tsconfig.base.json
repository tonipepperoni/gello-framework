      "@gello/platform-node": ["libs/publishable/platform-node/src/index.ts"],
      "@gello/queue": ["libs/publishable/queue/src/index.ts"],
      "@gello/queue-core": ["libs/queue/core/src/index.ts"],
      "@gello/queue-drivers": ["libs/queue/drivers/src/index.ts"],
      "@gello/queue-worker": ["libs/queue/worker/src/index.ts"],
